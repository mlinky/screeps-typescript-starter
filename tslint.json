{
  "rulesDirectory": "tslint-plugin-prettier",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "forin": false,
    "no-conditional-assignment": true,
    "max-classes-per-file":false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-ordering": [
      false
    ],
    "no-console": [
      false
    ],
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}
